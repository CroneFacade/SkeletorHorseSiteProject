@model IEnumerable<SkeletorDAL.Model.HorseVideoModel>

@foreach (var video in Model)
{
    <div><iframe title="YouTube video player" width="480" height="390" src="@video.VideoURL" frameborder="0"></iframe></div>
    @Html.ActionLink("Delete this video", "DeleteYoutubeVideoFromHorse", new { id=video.ID})
}

