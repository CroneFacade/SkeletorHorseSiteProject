@using System.Security.Policy
@model SkeletorDAL.Model.HorseModel

<div id="fb-root"></div>
<script>
    (function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/sv_SE/sdk.js#xfbml=1&version=v2.3";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
</script>

<div class="fb-page" data-href="@Model.FacebookPath" data-width="500" data-hide-cover="false" data-show-facepile="true" data-show-posts="true">

    <div class="fb-xfbml-parse-ignore">
        <blockquote cite="@Model.FacebookPath"><a href="@Model.FacebookPath"></a></blockquote>
    </div>
</div>